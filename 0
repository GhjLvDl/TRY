value |=1<<bit_number;
value &=~(1<<bit_number);

int
count_one_bits(unsigned value)
{
    int ones;
    for(ones=0;value!=0;value>>=1)
        if((value&1)!=0)
        ones+=1;
        
        
    return ones;
}

//
//

#include<stdlib>

size_t
strlen(char *string)
{
    int length=0;
    while(*string++!='\0')
        length+=1;
    
    return length;
}

//
//

#include<stdio.h>
#include<assert.h>

#define TURE  1
#define FALSE 0
int
find_char(char **strings,int value)
{
    assert(string!=NULL);
    while(*strings!=NULL){
        while(**strings!='\0'){
        
            if(*(*strings)++==value)
                return TURE;
        }
        strings++;
    }
    return FALSE;
}

